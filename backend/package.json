{
  "dependencies": {
    "@mysten/sui": "^1.45.2",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "module-alias": "^2.2.3",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0"
  },
  "_moduleAliases": {
    "@": "src",
    "@Domain": "src/Domain",
